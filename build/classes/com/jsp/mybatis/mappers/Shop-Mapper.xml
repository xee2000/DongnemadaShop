<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="Shop-Mapper">
	
	
	<select id="selectByShop_Id" parameterType="str" resultType="shop">
		select
		*
		from shop
		where shop_id=#{shop_id}
	</select>
	
	
</mapper>














